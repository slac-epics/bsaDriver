#
#   service status monitoring PVs
#

record(longin, "$(DEVICE):$(SERVICE):CURPKTSZ")
{
    field(DESC, "$(SERVICE) Current Packet Size")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT), 0) $(SERVICE)_currPacketSize")
    field(SCAN, "I/O Intr")
}

record(mbbi, "$(DEVICE):$(SERVICE):CURPKTST")
{
    field(DESC, "$(SERVICE) Current Packet Status")
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT), 0) $(SERVICE)_currPacketStatus")
    field(SCAN, "I/O Intr")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(THVL, "3")
    field(FRVL, "4")
    field(FVVL, "5")
    field(SXVL, "6")
    field(SVVL, "7")
    field(EIVL, "8")
    field(NIVL, "9")
    field(TEVL, "10")
    field(ELVL, "11")
    field(ZRST, "Idle")
    field(ONST, "TSL")
    field(TWST, "TSU")
    field(THST, "PIDL")
    field(FRST, "PIDU")
    field(FVST, "CHM")
    field(SXST, "DLT")
    field(SVST, "SVC")
    field(EIST, "CHD")
    field(NIST, "SEV")
    field(TEST, "END")
    field(ELST, "Invalid")

}



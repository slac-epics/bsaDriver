record(mbbo, "$(DEVICE):$(SERVICE):$(EDEF):MEASSEVR")
{
    field(PINI, "YES")
    field(DESC, "$(SERVICE) Seveirty Control")
    field(DTYP, "asynInt32")
    field(OUT, "@asyn($(PORT),0) $(SERVICE)_edefSevr_$(BSA)")
    field(DOL, "$(GLOBAL):$(EDEF):MEASSEVR_SET CPP NMS")
    field(OMSL, "closed_loop")
    field(SCAN, "Passive")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(THVL, "3")
    field(ZRST, "None")
    field(ONST, "Minor")
    field(TWST, "Major")
    field(THST, "Invalid")
    field(VAL, "2")
    info(autosave, "VAL")
}

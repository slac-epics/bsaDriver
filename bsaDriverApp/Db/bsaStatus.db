#   epics 7 will merge these duplicatd PVs are in one instance
#


record(bi, "$(TPR):BSA_STATUS")
{
    field(DESC, "BSA Status")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT), 0) status")
    field(SCAN, "I/O Intr")
    field(ZNAM, "NORMAL")
    field(ONAM, "FAULT")
    field(ZSV,  "NO_ALARM")
    field(OSV,  "MAJOR")
    field(PINI, "YES")
}



record(longin, "$(TPR):BSA_FLTCNT")
{
    field(DESC, "BSA Fault Count")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT), 0) fault_cnt")
    field(SCAN, "I/O Intr")
    field(PINI, "YES")
}


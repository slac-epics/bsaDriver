#
#    BLD specific PV template
#    macros: $(DEVICE)


record(stringout, "$(DEVICE):BLD$(BLD_RATEID):MULT_ADDR")
{
    field(DESC, "BLD $(BLD_RATEID) multicast address")
    field(DTYP, "asynOctetWrite")
    field(PINI, "YES")
    field(OUT,  "@asyn($(PORT), 0) bldMulticastAddress_$(IDX)")
    info(autosaveFields, "VAL")
}

record(longout, "$(DEVICE):BLD$(BLD_RATEID):MULT_PORT")
{
    field(DESC, "BLD $(BLD_RATEID) multicast port")
    field(DTYP, "asynInt32")
    field(PINI, "YES")
    field(OUT,  "@asyn($(PORT),0) bldMulticastPort_$(IDX)")
    info(autosaveFields, "VAL")
}
